{
  "name": "lab2view/blockchain-monitor",
  "description": "Manage Blockchain.info Wallet with API",
  "license": "MIT",
  "authors": [
    {
      "name": "Lab2View SARL",
      "email": "lab2view@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^7.0|^8.0",
    "blockchain/blockchain": "^1.4",
    "ext-bcmath": "*"
  },
  "autoload": {
    "psr-4": {
      "Lab2view\\BlockchainMonitor\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lab2view\\BlockchainMonitor\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lab2view\\BlockchainMonitor\\BlockchainServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/canvas": "^6.0@dev",
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.4@dev"
  }
}
